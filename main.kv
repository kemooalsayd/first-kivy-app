<Intm>:
    name:'introm'
    GridLayout:
        rows:6
        Label:
            text:'HOW ARE YOU'
            color:(12/255,126/255,200/255,1)
            size_hint:(1,.3)
        Label:
            text:'__________'*2
            font_size:30
            size_hint:(1,.1)
            color:(12/255,126/255,200/255,1)
        Image:
            source:'i.jpeg'
        Label:
            text:'__________'*2
            font_size:30
            size_hint:(1,.1)
            color:(12/255,126/255,200/255,1)
        Label:
            text:'welcome to our apps we happy to see you'
            color:(12/255,126/255,200/255,1)
            size_hint:(1,.3)


        GridLayout:
            cols:2
            size_hint:(1,.2)
            Button:
                text:'skip'
                background_color:(12/255,126/255,200/255,1)
                on_release:
                    app.root.current="main"
                    root.manager.transition.direction="up"
            Button:
                text:'next'
                background_color:(12/255,126/255,200/255,1)
                on_release:
                    app.root.current="introf"
                    root.manager.transition.direction="left"
