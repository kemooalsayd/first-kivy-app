<Siu>:
    name:"main"
    GridLayout:
        cols:1
        rows:4
        GridLayout:
            cols:1
            rows:2
            size_hint:(1,.5)
            padding:10
            Image:
                source:'icon.png'
            Label:
                text:'Please sign up'
                font_size:22
                color:(0,0,0,1)
        Label:
            id:inp
            text:'__________'*2
            font_size:30
            size_hint:(1,.1)
            color:(12/255,126/255,200/255,1)
        GridLayout:
            cols:1
            rows:3
            padding:20
            GridLayout:
                cols:1
                rows:2
                Label:
                    text:'enter your name :'
                    color:(12/255,126/255,200/255,1)
                TextInput:
                    id:nam
                    multiline:False
                    size_hint:(.9,.5)
            GridLayout:
                cols:1
                rows:2
                Label:
                    text:'enter your email :'
                    color:(12/255,126/255,200/255,1)
                TextInput:
                    id:ema
                    multiline:False
                    size_hint:(.9,.5)
            GridLayout:
                cols:1
                rows:2
                Label:
                    text:'enter your password :'
                    color:(12/255,126/255,200/255,1)
                TextInput:
                    id:pas
                    password:True
                    size_hint:(.9,.5)
        Button:
            text:'sign up'
            size_hint:(1,.2)
            background_color:(12/255,126/255,200/255,1)
            on_release:
                app.root.current="mapp"if pas.text=='123456' and nam.text=='kareem' and ema.text=='kemoo@gmail.com' else"ero"
                root.manager.transition.direction="up"
<Ero>:
    name:'ero'
    GridLayout:
